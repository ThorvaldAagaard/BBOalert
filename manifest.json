{
  "manifest_version":2,
  "name":"BBOalert",
  "description":"Auto alert on BBO",
  "version":"1.0",
  "permissions": [
    "storage",
    "clipboardRead",
    "clipboardWrite"],
  "content_scripts": [
    {
      "matches": [
        "*://www.bridgebase.com/*"],
      "js": [
        "BBOalert.js"]
    }
  ]
}
